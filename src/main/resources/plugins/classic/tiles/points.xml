        <apply>SI.CcRr SE</apply>
    </point>
    <point feature="ROAD" cx="750" cy="500">
        <apply>AM.RrC SE</apply>
    </point>
    <point feature="ROAD" cx="350" cy="750">
        <apply>AM.CRr SE</apply>
    </point>
    <point feature="ROAD" cx="820" cy="400">
        <apply>GQ.CcRR E</apply>
