    </point>
    <point feature="FARM" cx="180" cy="180">
        <apply>R2.IFC NR.ER</apply>
    </point>
    <point feature="FARM" cx="650" cy="150">
        <apply>R2.RIrI NR.EL</apply>
    </point>
    <point feature="FARM" cx="820" cy="820">
        <apply>KS.LC EL.ER.SL.SR.WL.WR</apply>
    </point>
    <point feature="FARM" cx="750" cy="750">
        <apply>TB.CCc.w INNER_FARM</apply>
        <apply>TB.CCc.c INNER_FARM</apply>
        <apply>TO.CccC+ INNER_FARM</apply>
    </point>
    <point feature="FARM" cx="750" cy="250">
        <apply>TB.LRRR NL.NR.EL.WR</apply>
    </point>
    <point feature="FARM" cx="180" cy="820">
        <apply>SI.RCr WL.SL.SR.ER</apply>
        <apply>TO.RrRr.2 NR.EL.SR.WL</apply>
