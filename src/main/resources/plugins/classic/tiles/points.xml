    </point>
    <point feature="ROAD" cx="500" cy="500">
        <apply>* NS</apply>
        <apply>* WE</apply>
        <apply>* _N</apply>
        <apply>* _E</apply>
        <apply>* _S</apply>
        <apply>* _W</apply>
    </point>

    <point feature="CASTLE" cx="500" cy="0" baseLocation="N">
        <apply>* N</apply>
        <apply>* E</apply>
        <apply>* S</apply>
        <apply>* W</apply>
    </point>

    <point feature="CITY" cx="500" cy="180" baseLocation="N">
        <apply>* N</apply>
        <apply>* E</apply>
        <apply>* S</apply>
        <apply>* W</apply>
    </point>
    <point feature="CITY" cx="370" cy="200" baseLocation="NW">
        <apply>* NW</apply>
        <apply>* NE</apply>
        <apply>* SE</apply>
        <apply>* SW</apply>
    </point>
    <point feature="CITY" cx="500" cy="800" baseLocation="_N">
        <apply>* _N</apply>
        <apply>* _E</apply>
        <apply>* _S</apply>
        <apply>* _W</apply>
