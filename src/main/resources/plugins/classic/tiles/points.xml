    </point>
    <point feature="ROAD" cx="800" cy="600">
        <apply>TU.RCR E</apply>
    </point>
    <point feature="ROAD" cx="500" cy="750">
        <apply>BB.LRFr WE</apply>
    </point>
    <point feature="ROAD" cx="720" cy="720" baseLocation="SE"> <!-- oblique -->
        <apply>BA.CcRr SE</apply>
        <apply>BA.CcRr+ SE</apply>
        <apply>CA.RrR.1 SE</apply>
        <apply>IC.CcRr+.i SE</apply>
        <apply>KS.CRrR SE</apply>
        <apply>DG.RrRr.g SE</apply>
        <apply>DG.CcRr.p SE</apply>
        <apply>TO.RrRr.2 SE</apply>
        <apply>GQ.CRRr SW</apply>
        <apply>IC.RrRr SW</apply>
        <apply>R1.RrII NE</apply>
        <apply>R2.RrII NE</apply>
        <apply>TO.RrRr.2 NW</apply>
    </point>

    <point feature="CITY" cx="300" cy="200">
        <apply>DG.CCc+.p N</apply>
    </point>
    <point feature="CITY" cx="500" cy="820" baseLocation="NS">
        <apply>KS.CCcc NS</apply>
        <apply>R2.CIcI WE</apply>
        <apply>KS.CCcc WE</apply>
        <apply>TO.CRcr WE</apply>
    </point>
    <point feature="CITY" cx="500" cy="250">
        <apply>AM.CCcc+ NS</apply>
    </point>
    <point feature="CITY" cx="750" cy="500">
        <apply>AM.CCcc+ WE</apply>
        <apply>AM.CCc+ WE</apply>
    </point>
    <point feature="CITY" cx="250" cy="500">
        <apply>AM.CRcr+ WE</apply>
    </point>
    <point feature="CITY" cx="350" cy="350">
        <apply>TB.CcRR!.c NW</apply>
        <apply>TB.CcR!.w NW</apply>
    </point>
    <point feature="CITY" cx="550" cy="550">
