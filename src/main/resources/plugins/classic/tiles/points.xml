    </point>
    <point feature="CLOISTER" cx="180" cy="330">
        <apply>DG.LCcc.d CLOISTER</apply>
    </point>
    <point feature="CLOISTER" cx="300" cy="700">
        <apply>TO.L CLOISTER</apply>
    </point>
    <point feature="CLOISTER" cx="340" cy="400">
        <apply>CA.L CLOISTER</apply>
    </point>

    <point feature="ROAD" cx="300" cy="820">
        <apply>R1.LIRI S</apply>
    </point>
    <point feature="ROAD" cx="500" cy="250" baseLocation="NS"> <!-- vertical road, but not allowed in center -->
        <apply>R1.RIrI NS</apply>
        <apply>R2.RIrI WE</apply>
        <apply>TB.RRrr NS</apply>
        <apply>TB.RRrr WE</apply>
        <apply>TO.CRcr NS</apply>
    </point>
    <point feature="ROAD" cx="500" cy="820" baseLocation="NS"> <!-- vertical road, with only short parts near border -->
        <apply>DG.CRcr.d NS</apply>
        <apply>TO.RCr WE</apply>
        <apply>AM.CRcr+ NS</apply>
    </point>
    <point feature="ROAD" cx="200" cy="500" baseLocation="W">
        <apply>TB.RRC W</apply>
        <apply>TB.RRC S</apply>
    </point>
    <point feature="ROAD" cx="850" cy="400">
        <apply>KS.CcRR! E</apply>
